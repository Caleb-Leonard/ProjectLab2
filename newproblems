async def main():
    await start_gui_thread()
    
    await asyncio.create_task(update_sensors())
    
    
    if picked_room == "Room 1":
        task1 = asyncio.create_task(cooling1())
        await task1
    
    if picked_room == "Room 2":
        task1.cancel()
        task2 = asyncio.create_task(Heating2())
        await task2
